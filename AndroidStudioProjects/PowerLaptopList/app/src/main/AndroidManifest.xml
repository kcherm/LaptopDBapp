<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.powerlaptoplist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="PowerLaptopList"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.laptoplist">
        <activity
            android:name="com.example.powerlaptoplist.laptoplistActivity"
            android:exported="true" />
        <activity
            android:name="com.example.powerlaptoplist.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>